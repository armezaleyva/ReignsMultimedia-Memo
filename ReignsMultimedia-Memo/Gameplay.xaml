<UserControl x:Class="ReignsMultimedia_Memo.Gameplay"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ReignsMultimedia_Memo"
             mc:Ignorable="d" 
             d:DesignHeight="750" d:DesignWidth="400">
    <Grid>
        <StackPanel Margin="0,0,0,660.4" Background="#FF202020" d:IsLocked="True"/>
        <StackPanel Margin="0,90,0,0.4" Background="#FF2C2C2C">
            <TextBlock Name="lblEvent" TextWrapping="Wrap" Text="TextBlock" Foreground="#FFEBEBEB"/>
        </StackPanel>
    </Grid>
</UserControl>
